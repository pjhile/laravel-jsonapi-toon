{
    "name": "pjhile/laravel-jsonapi-toon",
    "description": "TOON (Token-Oriented Object Notation) support for laravel-json-api/laravel",
    "keywords": ["laravel", "jsonapi", "jsonapi", "toon", "llm", "ai"],
    "license": "MIT",
    "require": {
        "php": "^8.2|^8.3",
        "laravel-json-api/laravel": "^5.0",
        "helgesverre/toon": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Pjhile\\JsonApiToon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pjhile\\JsonApiToon\\JsonApiToonServiceProvider"
            ]
        }
    }
}
